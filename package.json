{
  "name": "@petkit-io/material-table",
  "version": "1.0.2",
  "description": "material table",
  "main": "index.js",
  "scripts": {
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "build": "sh ./bin/build.sh",
    "start": "ng serve --port 5200",
    "prepub:dev": "npm run build",
    "pub:dev": "npm publish dist --access public --tag dev",
    "postpub:dev": "npm version prerelease && git push && git push --tags",
    "prepub": "npm run build",
    "pub": "npm publish dist --access public",
    "postpub": "npm version patch && git push && git push --tags"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "umdModuleIds": {}
    },
    "whitelistedNonPeerDependencies": []
  },
  "repository": "git@github.com:petkit-io/material-table.git",
  "author": "liuxin <475212506@qq.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@angular/animations": "6.1.0",
    "@angular/cdk": "6.1.0",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/compiler-cli": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/material": "6.1.0",
    "@angular/platform-browser": "6.1.0",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "hammerjs": "^2.0.8",
    "husky": "^0.14.3",
    "lodash-es": "^4.17.7",
    "ng-packagr": "^3.0.6",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "6.1.0",
    "tsickle": "^0.32.1",
    "tslib": "^1.9.3",
    "typescript": "2.7.2",
    "zone.js": "0.8.26"
  },
  "peerDependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/material": "^6.0.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.7",
    "moment-es6": "^1.0.0",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.2.2",
    "typescript": "^2.7.2",
    "xlsx": "^0.13.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
